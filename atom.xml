<span class='line-number'>23</span>
</pre></td><td class='code'><pre><code class='python'><span class='line'><span class="k">def</span> <span class="nf">shop_fast</span><span class="p">(</span><span class="n">user</span><span class="p">,</span> <span class="n">items_to_buy</span><span class="p">):</span>
</span><span class='line'>    <span class="n">shopping_cart_items</span> <span class="o">=</span> <span class="p">[]</span>
</span><span class='line'>    <span class="k">for</span> <span class="n">item</span> <span class="ow">in</span> <span class="n">items_to_buy</span><span class="p">:</span>
</span><span class='line'>        <span class="n">item</span><span class="o">.</span><span class="n">find</span><span class="p">()</span>
</span><span class='line'>        <span class="k">if</span> <span class="n">user</span><span class="o">.</span><span class="n">wants</span><span class="p">(</span><span class="n">item</span><span class="p">):</span>
</span><span class='line'>            <span class="n">shopping_cart_items</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">item</span><span class="p">)</span>
</span><span class='line'>    <span class="k">for</span> <span class="n">item</span> <span class="ow">in</span> <span class="n">shopping_cart_items</span><span class="p">:</span>
</span><span class='line'>        <span class="n">user</span><span class="o">.</span><span class="n">buy</span><span class="p">(</span><span class="n">item</span><span class="p">)</span>
</span><span class='line'>
</span><span class='line'><span class="k">def</span> <span class="nf">shop_slow</span><span class="p">(</span><span class="n">user</span><span class="p">,</span> <span class="n">items_to_buy</span><span class="p">):</span>
</span><span class='line'>    <span class="n">shopping_cart_items</span> <span class="o">=</span> <span class="p">[]</span>
</span><span class='line'>    <span class="k">for</span> <span class="n">item</span> <span class="ow">in</span> <span class="n">items_in_store</span><span class="p">:</span>
</span><span class='line'>        <span class="k">if</span> <span class="n">item</span> <span class="ow">in</span> <span class="n">items_to_buy</span><span class="p">:</span>
</span><span class='line'>            <span class="k">print</span><span class="p">(</span><span class="s">&#39;Awesome!&#39;</span><span class="p">)</span>
</span><span class='line'>        <span class="k">if</span> <span class="n">user</span><span class="o">.</span><span class="n">wants</span><span class="p">(</span><span class="n">item</span><span class="p">):</span>
</span><span class='line'>            <span class="n">shopping_cart_items</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">item</span><span class="p">)</span>
</span><span class='line'>    <span class="k">for</span> <span class="n">item</span> <span class="ow">in</span> <span class="n">shopping_cart_items</span><span class="p">:</span>
</span><span class='line'>        <span class="n">user</span><span class="o">.</span><span class="n">buy</span><span class="p">(</span><span class="n">item</span><span class="p">)</span>
</span><span class='line'>
</span><span class='line'><span class="k">if</span> <span class="n">gender</span> <span class="ow">is</span> <span class="s">&#39;Male&#39;</span><span class="p">:</span>
</span><span class='line'>    <span class="n">shop_fast</span><span class="p">()</span>
</span><span class='line'><span class="k">else</span><span class="p">:</span>
</span><span class='line'>    <span class="n">shop_slow</span><span class="p">()</span>
</span></code></pre></td></tr></table></div></figure>

]]></content>
